{
  "name": "101obex-api-extension-api-specs",
  "displayName": "AVAP API Specs",
  "description": "AVAP API Specs",
  "version": "1.30.75",
  "publisher": "101OBeXCorp",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Other"
  ],
  "extensionPack": [],
  "extensionDependencies": [
  ],
  "recommendations": [
    "101OBeXCorp.101obex-api-extension"
  ],
  "icon": "images/AVAP_Specs.png",
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "101obex-api-specs",
          "title": "AVAP API Specs",
          "icon": "images/AVAP-Specs-VC.svg"
        }
      ]
    },
    "views": {
      "101obex-api-specs": [
        {
          "id": "101obex-api-specs.api-specs",
          "name": "AVAP API Specs",
          "type": "tree"
        }
      ]
    },
    "configuration": {
      "title": "AVAP Specs",
      "type": "object",
      "properties": {
        "avap.Specs.mandatory": {
          "type": "boolean",
          "default": false,
          "description": "Show specs from url instead of own projects specs",
          "order": 8
        },
        "avap.Specs.url": {
          "type": "string",
          "default": "url",
          "description": "Project url Specs",
          "order": 9
        },
        "avap.Specs.authtoken": {
          "type": "string",
          "default": " ",
          "description": "Authorization token to test paths",
          "order": 10
        }

      }
    },
    "commands": [
      {
        "command": "101obex-api-extension-specs.openSettings",
        "title": "Open AVAP Specs configuration",
        "icon": "$(settings)"
      },
      {
        "command": "101obex-api-extension-api-specs.viewPath",
        "title":"view Path Specs",
        "icon": "images/AVAP-Specs-llaves.svg"
      },
      {
        "command": "react-webview-creation.start",
        "title": "Start React Webview",
        "category": "React",
        "icon": "$(refresh)"
      },
      {
        "command": "101obex-api-extension-api-specs.refreshEntry-api-creator",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "101obex-api-extension-api-specs.addAPI-api-creator",
        "title": "Add new API",
        "icon": "$(plus)"
      },
      {
        "command": "101obex-api-extension-api-specs.commitPush-api-creator",
        "title": "Push Commit",
        "icon": "$(repo-push)"
      },
      {
        "command": "101obex-api-extension-api-specs.checkout-api-creator",
        "title": "commit checkout",
        "icon": "$(repo-push)"
      },

      {
        "command": "101obex-api-extension-api-specs.CloneAPI",
        "title": "Clone API",
        "icon": "$(repo-clone)"
      },

      {
        "command": "101obex-api-extension-api-specs.CheckoutAPI",
        "title": "Checkout this Commit"
      },
      {
        "command": "101obex-api-extension-api-specs.eraseAPI",
        "title": "Erase API",
        "icon": "$(terminal-kill)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command":"101obex-api-extension-specs.openSettings",
          "when":"view == 101obex-api-specs.api-specs",
          "group": "navigation"
        },
        {
          "command": "101obex-api-extension-api-specs.refreshEntry-api-creator",
          "when": "view == 101obex-api-specs.api-specs",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "101obex-api-extension-api-specs.eraseAPI",
          "when": "view ==  101obex-api-specs.api-specs && viewItem == UNPUBLISHED",
          "group": "inline"
        },
        {
          "command": "101obex-api-extension-api-specs.viewPath",
          "when": "view ==  101obex-api-specs.api-specs && viewItem == PATH",
          "group": "inline"
        },
        {
          "command": "101obex-api-extension-api-specs.CloneAPI",
          "when": "view == 101obex-api-specs.api-specs && (viewItem == APICONF || viewItem == UNPUBLISHED)"
        },
        
        {
          "command": "101obex-api-extension-api-specs.CloneAPI",
          "when": "view == 101obex-api-specs.api-specs && (viewItem == APICONF || viewItem == UNPUBLISHED)",
          "group": "inline"
        },
        {
          "command": "101obex-api-extension-api-specs.CloneAPI",
          "when": "view == 101obex-api-specs.api-specs && viewItem == APICONF",
          "group": "inline"
        },
        {
          "command": "101obex-api-extension-api-specs.CheckoutAPI",
          "when": "view == 101obex-api-specs.api-specs && viewItem == COMMITCONF"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@types/request": "^2.48.8",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.26.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "axios": "^1.2.0",
    "chatgpt": "^5.1.2",
    "expiry-map": "^2.0.0",
    "gpt3-tokenizer": "^1.1.5",
    "node-datetime": "^2.1.2",
    "node-fetch": "^3.3.1",
    "request": "^2.88.2",
    "requests": "^0.3.0",
    "vsce": "^2.15.0",
    "vscode-uri": "^3.0.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/101OBeXCorp/101obex_vscode_extension.git"
  }
}
