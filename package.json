{
  "name": "101obex-api-extension",
  "displayName": "AVAP DEV Extensions for Microsoft Visual Studio",
  "description": "AVAP DEV Extensions for Microsoft Visual Studio",
  "version": "1.30.80",
  "publisher": "101OBeXCorp",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "images/icono_DS.png",
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "101obex-api-extension",
          "title": "AVAP DEV Extensions for Microsoft Visual Studio",
          "icon": "images/icono_avap.png"
        }
      ]
    },
    "views": {
      "101obex-api-extension": [
        {
          "id": "package-organizations",
          "name": "Deployment and Organizations",
          "type": "tree"
        },
        {
          "id": "package-teams",
          "name": "Developer Teams",
          "type": "tree"
        },
        {
          "id": "package-projects",
          "name": "API Projects",
          "type": "tree"
        }
      ]
    },
    "commands": [
      {
        "command": "101obex-api-extension.refreshEntry-organizations",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "101obex-api-extension.select-organizations",
        "title": "Activate",
        "icon": "$(check)"
      },
      {
        "command": "101obex-api-extension.select-deployment",
        "title": "Activate",
        "icon": "$(check)"
      },
      {
        "command": "101obex-api-extension.copy-address",
        "title": "Copy",
        "icon": "$(files)"
      },
      {
        "command": "101obex-api-extension.copy-token",
        "title": "Copy",
        "icon": "$(files)"
      },
      {
        "command": "101obex-api-extension.refreshEntry-projects",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "101obex-api-extension.refreshEntry-teams",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
				"command": "run-button-script.run",
				"title": "Run script button",
				"icon": {
					"light": "./images/play-icon.svg",
					"dark": "./images/play-icon.svg"
				}
			}
    ],
    "menus": {
      "editor/title": [
				{
					"command": "run-button-script.run",
					"when": "resourceLangId == avap ",
					"group": "navigation"
				}
			],
      "view/title": [
        {
          "command": "101obex-api-extension.refreshEntry-organizations",
          "when": "view == package-organizations",
          "group": "navigation"
        },
        {
          "command": "101obex-api-extension.select-organizations",
          "when": "view == package-organizations && viewItem == ORGANIZATION",
          "group": "inline"
        },
        {
          "command": "101obex-api-extension.copy-address",
          "when": "viewItem == COPYADDRESS",
          "group": "inline"
        },
        {
          "command": "101obex-api-extension.copy-token",
          "when": "view == package-projects && viewItem == COPYTOKEN",
          "group": "inline"
        },
        {
          "command": "101obex-api-extension.select-deployment",
          "when": "view == package-organizations && viewItem == DEPLOYMENT",
          "group": "inline"
        },

        {
          "command": "101obex-api-extension.refreshEntry-teams",
          "when": "view == package-teams",
          "group": "navigation"
        },
        {
          "command": "101obex-api-extension.refreshEntry-projects",
          "when": "view == package-projects",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "101obex-api-extension.select-organizations",
          "when": "viewItem == ORGANIZATION"
        },
        {
          "command": "101obex-api-extension.select-organizations",
          "when": "view == package-organizations && viewItem == ORGANIZATION",
          "group": "inline"
        },
        {
          "command": "101obex-api-extension.copy-address",
          "when": "viewItem == COPYADDRESS",
          "group": "inline"
        }
        ,
        {
          "command": "101obex-api-extension.copy-token",
          "when": "view == package-projects && viewItem == COPYTOKEN",
          "group": "inline"
        }
        ,
        {
          "command": "101obex-api-extension.select-deployment",
          "when": "view == package-organizations && viewItem == DEPLOY",
          "group": "inline"
        }
      ],
      "editor/context": [

      ],
      "commandPalette": [
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@types/request": "^2.48.8",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.26.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "axios": "^1.2.0",
    "chatgpt": "^5.1.2",
    "expiry-map": "^2.0.0",
    "gpt3-tokenizer": "^1.1.5",
    "node-datetime": "^2.1.2",
    "node-fetch": "^3.3.1",
    "request": "^2.88.2",
    "requests": "^0.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/101OBeXCorp/101obex_vscode_extension.git"
  }
}
