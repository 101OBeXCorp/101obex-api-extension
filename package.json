{
  "name": "101obex-api-extension-ia",
  "displayName": "AVAP AI aka Brunix",
  "description": "AVAP AI aka Brunix",
  "version": "0.0.5",
  "publisher": "101OBeXCorp",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Other"
  ],
  "extensionPack": [
   
],
"extensionDependencies":["101OBeXCorp.101obex-api-extension"],
"recommendations": ["101OBeXCorp.101obex-api-extension"],
  "icon": "images/icono_AI.png",
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "101obex-api-extension-ia",
          "title": "AVAP AI aka Brunix",
          "icon": "images/brunixicon.png"
        }
      ]
    },
    "views": {
      "101obex-api-extension-ia": [
        {
          "type": "webview",
          "id": "101obex-api-extension-ia.chatView",
          "name": "Chat"
        },
        {
          "type": "tree",
          "id": "101obex-api-extension-ia.previous",
          "name": "Previous Chats"
        }
      ]
    },
    "configuration": {
      "title": "Brunix",
      "type": "object",
      "properties": {
        "brunix.openAI.apikey": {
          "type": "string",
          "default": "apikey",
          "description": "OpenAI Api Key",
          "order": 8
        }
      }
    },
    "commands": [

      {
        "command": "101obex-api-extension-ia.chatRecover",
        "title": "Recover Chat",
        "icon": "$(comment)"
      },
      {
        "command": "101obex-api-extension-ia.editChatName",
        "title": "Edit chat name",
        "icon":"$(pencil)"
      },
      {
        "command": "101obex-api-extension-ia.deleteChat",
        "title": "Delete Chat",
        "icon":"$(terminal-kill)"
      },

      {
        "command": "101obex-api-extension-ia.recoverChats",
        "title": "Recover Chats",
        "icon": "$(refresh)"
      },
      {
        "command": "extension.showCodeProposal",
        "title": "Show Proposition"
      },
      {
        "command": "101obex-api-extension-ia-recibe.ask",
        "title": "recive ask"
      },
      {
        "command": "101obex-api-extension-ia.ask",
        "title": "Ask Brunix"
      },
      {
        "command": "101obex-api-extension-ia.explain",
        "title": "Brunix: Explain selection"
      },
      {
        "command": "101obex-api-extension-ia.refactor",
        "title": "Brunix: Refactor selection"
      },
      {
        "command": "101obex-api-extension-ia.findProblems",
        "title": "Brunix: Find problems"
      },
      {
        "command": "101obex-api-extension-ia.optimize",
        "title": "Brunix: Optimize selection"
      },
      {
        "command": "101obex-api-extension-ia.documentation",
        "title": "Brunix: Write documentation"
      },
      {
        "command": "101obex-api-extension-ia.saveChat",
        "title": "Save Brunix chat",
        "icon": "$(save)"
      },
      {
        "command": "101obex-api-extension-ia.resetConversation",
        "title": "Reset Brunix chat",
        "icon": "$(terminal-kill)"
      },

      {
        "command": "101obex-api-extension-ia.tryAgain",
        "title": "Retry Brunix request"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "101obex-api-extension-ia.recoverChats",
          "when": "view == 101obex-api-extension-ia.previous",
          "group": "navigation@0"
        },
        {
          "command": "101obex-api-extension-ia.saveChat",
          "when": "view == 101obex-api-extension-ia.chatView",
          "group": "navigation@0"
        },
        {
          "command": "101obex-api-extension-ia.resetConversation",
          "when": "view == 101obex-api-extension-ia.chatView",
          "group": "navigation@1"
        }
      
  
      ],
      "view/item/context": [
        {
          "command": "101obex-api-extension-ia.resetConversation",
          "when": "view == 101obex-api-extension-ia.chatView",
          "group": "inline"
        },
        {
          "command": "101obex-api-extension-ia.chatRecover",
          "when": "view == 101obex-api-extension-ia.previous",
          "group": "inline@0"
        },
        {
          "command": "101obex-api-extension-ia.editChatName",
          "when": "view == 101obex-api-extension-ia.previous",
          "group": "inline@1"
        },
        {
          "command": "101obex-api-extension-ia.deleteChat",
          "when": "view == 101obex-api-extension-ia.previous",
          "group": "inline@2"
        }
      ],
      "editor/context": [
        {
          "command": "101obex-api-extension-ia.resetConversation",
          "when": "view == 101obex-api-extension-ia.chatView",
          "group": "inline"
        },
        
        {
          "command": "101obex-api-extension-ia.ask",
          "when": "editorTextFocus",
          "group": "chatgpt-menu-group@1"
        },
        {
          "command": "extension.showCodeProposal",
          "when": "editorTextFocus",
          "group": "chatgpt-menu-group@7"
        },
        {
          "command": "101obex-api-extension-ia.explain",
          "when": "editorHasSelection",
          "group": "chatgpt-menu-group@2"
        },
        {
          "command": "101obex-api-extension-ia.refactor",
          "when": "editorHasSelection",
          "group": "chatgpt-menu-group@3"
        },
        {
          "command": "101obex-api-extension-ia.findProblems",
          "when": "editorHasSelection",
          "group": "chatgpt-menu-group@4"
        },
        {
          "command": "101obex-api-extension-ia.optimize",
          "when": "editorHasSelection",
          "group": "chatgpt-menu-group@5"
        },
        {
          "command": "101obex-api-extension-ia.documentation",
          "when": "editorHasSelection",
          "group": "chatgpt-menu-group@6"
        }
      ],
      "commandPalette": [
        {
          "command": "101obex-api-extension-ia.ask"
        },
        {
          "command": "extension.showCodeProposal"
        },
        {
          "command": "101obex-api-extension-ia.explain",
          "when": "editorHasSelection"
        },
        {
          "command": "101obex-api-extension-ia.refactor",
          "when": "editorHasSelection"
        },
        {
          "command": "101obex-api-extension-ia.findProblems",
          "when": "editorHasSelection"
        },
        {
          "command": "101obex-api-extension-ia.optimize",
          "when": "editorHasSelection"
        },
        {
          "command": "101obex-api-extension-ia.documentation",
          "when": "editorHasSelection"
        },
        {
          "command": "101obex-api-extension-ia.resetConversation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@types/request": "^2.48.8",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.26.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "axios": "^1.2.0",
    "chatgpt": "^5.1.2",
    "expiry-map": "^2.0.0",
    "gpt3-tokenizer": "^1.1.5",
    "node-datetime": "^2.1.2",
    "node-fetch": "^3.3.1",
    "request": "^2.88.2",
    "requests": "^0.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/101OBeXCorp/101obex_vscode_extension.git"
  }
}
