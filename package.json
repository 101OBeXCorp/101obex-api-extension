{
  "name": "101obex-api-extension-api-code",
  "displayName": "AVAP API Version Control",
  "description": "AVAP API Version Control",
  "version": "1.30.80",
  "publisher": "101OBeXCorp",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Other"
  ],
  "extensionPack": [],
  "extensionDependencies": [
    "101OBeXCorp.101obex-api-extension"
  ],
  "recommendations": [
    "101OBeXCorp.101obex-api-extension"
  ],
  "icon": "images/AVAP_APIControVersion.png",
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "101obex-api-code",
          "title": "AVAP API Control Version",
          "icon": "images/icono_avap.png"
        }
      ]
    },
    "views": {
      "101obex-api-extension": [
        {
          "id": "101obex-api-code.package-creation",
          "name": "AVAP API Version Control",
          "type": "tree"
        }
      ]
    },
    "commands": [
      {
        "command": "react-webview-creation.start",
        "title": "Start React Webview",
        "category": "React",
        "icon": "$(refresh)"
      },
      {
        "command": "101obex-api-extension-api-creation.refreshEntry-api-creator",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "101obex-api-extension-api-creation.addAPI-api-creator",
        "title": "Add new API",
        "icon": "$(plus)"
      },
      {
        "command": "101obex-api-extension-api-creation.commitPush-api-creator",
        "title": "Push Commit",
        "icon": "$(repo-push)"
      },
      {
        "command": "101obex-api-extension-api-creation.checkout-api-creator",
        "title": "commit checkout",
        "icon": "$(repo-push)"
      },

      {
        "command": "101obex-api-extension-api-creation.CloneAPI",
        "title": "Clone API",
        "icon": "$(repo-clone)"
      },

      {
        "command": "101obex-api-extension-api-creation.CheckoutAPI",
        "title": "Checkout this Commit"
      },
      {
        "command": "101obex-api-extension-api-creation.eraseAPI",
        "title": "Erase API",
        "icon": "$(terminal-kill)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "101obex-api-extension-api-creation.refreshEntry-api-creator",
          "when": "view == 101obex-api-code.package-creation",
          "group": "navigation"
        },
        {
          "command": "101obex-api-extension-api-creation.addAPI-api-creator",
          "when": "view == 101obex-api-code.package-creation",
          "group": "navigation"
        },
        {
          "command": "101obex-api-extension-api-creation.commitPush-api-creator",
          "when": "view == 101obex-api-code.package-creation",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "101obex-api-extension-api-creation.eraseAPI",
          "when": "view ==  101obex-api-code.package-creation && viewItem == UNPUBLISHED",
          "group": "inline"
        },
        {
          "command": "101obex-api-extension-api-creation.CloneAPI",
          "when": "view == 101obex-api-code.package-creation && viewItem == APICONF"
        },
        {
          "command": "101obex-api-extension-api-creation.CloneAPI",
          "when": "view == 101obex-api-code.package-creation && viewItem == APICONF",
          "group": "inline"
        },
        {
          "command": "101obex-api-extension-api-creation.CheckoutAPI",
          "when": "view == 101obex-api-code.package-creation && viewItem == COMMITCONF"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@types/request": "^2.48.8",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.26.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "axios": "^1.2.0",
    "chatgpt": "^5.1.2",
    "expiry-map": "^2.0.0",
    "gpt3-tokenizer": "^1.1.5",
    "node-datetime": "^2.1.2",
    "node-fetch": "^3.3.1",
    "request": "^2.88.2",
    "requests": "^0.3.0",
    "vsce": "^2.15.0",
    "vscode-uri": "^3.0.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/101OBeXCorp/101obex_vscode_extension.git"
  }
}
