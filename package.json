{
  "name": "101obex-api-extension",
  "displayName": "101OBeX API Extension",
  "description": "101OBeX API support extension",
  "version": "0.0.3",
  "publisher": "101OBeX Corp",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "images/101icon.png",
  "activationEvents": [
    "onStartupFinished",
    "onView:101obex-api-extension.chatView",
    "onCommand:101obex-api-extension.ask",
    "onCommand:101obex-api-extension.explain",
    "onCommand:101obex-api-extension.refactor",
    "onCommand:101obex-api-extension.optimize",
    "onCommand:101obex-api-extension.findProblems",
    "onCommand:101obex-api-extension.documentation"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "101obex-api-extension",
          "title": "101OBeX API support extension",
          "icon": "images/101icon.png"
        }
      ]
    },
    "views": {
      "101obex-api-extension": [
        {
          "id": "package-organizations",
          "name": "Organizations",
          "type": "tree"
        },
        {
          "id": "package-teams",
          "name": "Developer Teams",
          "type": "tree"
        },
        {
          "id": "package-projects",
          "name": "Projects",
          "type": "tree"
        },
        {
          "id": "package-APIs",
          "name": "APIs",
          "type": "tree"
        },
        {
          "type": "webview",
          "id": "101obex-api-extension.chatView",
          "name": "Marieta"
        }
      ]
    },
    "configuration": {
      "title": "Marieta",
      "type": "object",
      "properties": {
        "101obex-api-extension.apiKey": {
          "markdownDescription": "OpenAI API key from [https://platform.openai.com/account/api-keys](https://platform.openai.com/account/api-keys)",
          "type": "string",
          "order": 0
        },
        "101obex-api-extension.pasteOnClick": {
          "type": "boolean",
          "default": true,
          "description": "Paste the code from a codeblock inside the response into the editor when you click on it",
          "order": 2
        },
        "101obex-api-extension.promptPrefix.explain": {
          "type": "string",
          "default": "Explain what this code does: ",
          "description": "The prompt prefix used for explaining the selected code",
          "order": 3
        },
        "101obex-api-extension.promptPrefix.refactor": {
          "type": "string",
          "default": "Refactor this code and explain what's changed: ",
          "description": "The prompt prefix used for refactoring the selected code",
          "order": 4
        },
        "101obex-api-extension.promptPrefix.findProblems": {
          "type": "string",
          "default": "Find problems with the following code, fix them and explain what was wrong (Do not change anything else, if there are no problems say so): ",
          "description": "The prompt prefix used for finding problems in the selected code",
          "order": 5
        },
        "101obex-api-extension.promptPrefix.documentation": {
          "type": "string",
          "default": "Write documentation for the following code: ",
          "description": "The prompt prefix used for writing documentation for the selected code",
          "order": 7
        },
        "101obex-api-extension.promptPrefix.optimize": {
          "type": "string",
          "default": "Optimize the following code if there is anything to improve, if not say so: ",
          "description": "The prompt prefix used for optimizing the selected code",
          "order": 6
        },
        "101obex-api-extension.keepConversation": {
          "type": "boolean",
          "default": true,
          "description": "Keep the conversation going by using the same conversation ID for all requests (allows follow-up questions)",
          "order": 8
        },
        "101obex-api-extension.timeoutLength": {
          "type": "number",
          "default": "120",
          "description": "How long should the request wait for a response before timing out (in seconds)",
          "order": 9
        },
        "101obex-api-extension.selectedInsideCodeblock": {
          "type": "boolean",
          "default": true,
          "description": "Append selected code as a codeblock (```...code...```) instead of plain text",
          "order": 10
        },
        "101obex-api-extension.codeblockWithLanguageId": {
          "type": "boolean",
          "default": true,
          "description": "Append language id of the selected code to the codeblock (```language...code...```)",
          "order": 11
        }
      }
    },
    "commands": [
      {
        "command": "101obex-api-extension.refreshEntry-apis",
        "title": "Refresh"
      },
      {
        "command": "101obex-api-extension.refreshEntry-organizations",
        "title": "Refresh"
      },
      {
        "command": "101obex-api-extension.refreshEntry-projects",
        "title": "Refresh"
      },
      {
        "command": "101obex-api-extension.refreshEntry-teams",
        "title": "Refresh"
      },
      {
        "command": "101obex-api-extension.viewOnlineDocumentation",
        "title": "View online Documentation"
      },
      {
        "command": "101obex-api-extension.ask",
        "title": "Ask Marieta"
      },
      {
        "command": "101obex-api-extension.explain",
        "title": "ChatGPT: Explain selection"
      },
      {
        "command": "101obex-api-extension.refactor",
        "title": "ChatGPT: Refactor selection"
      },
      {
        "command": "101obex-api-extension.findProblems",
        "title": "ChatGPT: Find problems"
      },
      {
        "command": "101obex-api-extension.optimize",
        "title": "ChatGPT: Optimize selection"
      },
      {
        "command": "101obex-api-extension.documentation",
        "title": "ChatGPT: Write documentation"
      },
      {
        "command": "101obex-api-extension.resetConversation",
        "title": "Reset ChatGPT conversation"
      },
      {
        "command": "101obex-api-extension.tryAgain",
        "title": "Retry ChatGPT request"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "101obex-api-extension.refreshEntry-apis",
          "when": "view == package-APIs",
          "group": "navigation"
        },
        {
          "command": "101obex-api-extension.refreshEntry-organizations",
          "when": "view == package-organizations",
          "group": "navigation"
        },
        {
          "command": "101obex-api-extension.refreshEntry-teams",
          "when": "view == package-teams",
          "group": "navigation"
        },
        {
          "command": "101obex-api-extension.refreshEntry-projects",
          "when": "view == package-projects",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "101obex-api-extension.viewOnlineDocumentation",
          "when": "view == package-APIs"
        }
      ],
      "editor/context": [
        {
          "command": "101obex-api-extension.ask",
          "when": "editorTextFocus",
          "group": "chatgpt-menu-group@1"
        },
        {
          "command": "101obex-api-extension.explain",
          "when": "editorHasSelection",
          "group": "chatgpt-menu-group@2"
        },
        {
          "command": "101obex-api-extension.refactor",
          "when": "editorHasSelection",
          "group": "chatgpt-menu-group@3"
        },
        {
          "command": "101obex-api-extension.findProblems",
          "when": "editorHasSelection",
          "group": "chatgpt-menu-group@4"
        },
        {
          "command": "101obex-api-extension.optimize",
          "when": "editorHasSelection",
          "group": "chatgpt-menu-group@5"
        },
        {
          "command": "101obex-api-extension.documentation",
          "when": "editorHasSelection",
          "group": "chatgpt-menu-group@6"
        }
      ],
      "commandPalette": [
        {
          "command": "101obex-api-extension.ask"
        },
        {
          "command": "101obex-api-extension.explain",
          "when": "editorHasSelection"
        },
        {
          "command": "101obex-api-extension.refactor",
          "when": "editorHasSelection"
        },
        {
          "command": "101obex-api-extension.findProblems",
          "when": "editorHasSelection"
        },
        {
          "command": "101obex-api-extension.optimize",
          "when": "editorHasSelection"
        },
        {
          "command": "101obex-api-extension.documentation",
          "when": "editorHasSelection"
        },
        {
          "command": "101obex-api-extension.resetConversation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@types/request": "^2.48.8",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.26.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "axios": "^1.2.0",
    "chatgpt": "^5.1.2",
    "expiry-map": "^2.0.0",
    "gpt3-tokenizer": "^1.1.5",
    "node-datetime": "^2.1.2",
    "node-fetch": "^3.3.1",
    "request": "^2.88.2",
    "requests": "^0.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/101OBeXCorp/101obex_vscode_extension.git"
  }
}
